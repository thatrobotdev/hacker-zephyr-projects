{
  "name": "Zoogle Search Engine",
  "version": "0.0.1",
  "description": "Zoogle is love, zoogle is life.",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "z"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "is_default": true,
      "keyword": "zoogle.zephyr",
      "name": "Zoogle",
      "search_url": "https://zoogle.zephyr/q/{searchTerms}"
    }
  },
  "permissions": []
}